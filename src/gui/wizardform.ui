                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>domainLineEdit</cstring>
                    </property>
                </widget>
                <widget class="QComboBox" row="0" column="1">
                    <property name="name">
                        <cstring>serviceProviderComboBox</cstring>
                    </property>
                    <property name="whatsThis" stdset="0">
