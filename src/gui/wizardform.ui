                    </property>
                    <property name="text">
                        <string>SIP pro&amp;xy:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>proxyLineEdit</cstring>
                    </property>
                </widget>
                <widget class="QLineEdit" row="7" column="1" rowspan="1" colspan="2">
                    <property name="name">
                        <cstring>proxyLineEdit</cstring>
                    </property>
                    <property name="enabled">
                        <bool>true</bool>
                    </property>
